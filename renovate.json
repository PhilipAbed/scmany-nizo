{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:base",
		":disableRateLimiting",
		"local>PhilipAbed/renovate-config:default"
	],
	"packageRules": [ 
		{
			"matchDatasources": [
				"npm"
			],
			"labels": [
				"security fix"
			],
			"groupName": "npm group"
		},
		{
			"matchDatasources": [
				"pypi"
			],
			"labels": [
				"security fix"
			]
		},
		{
			"matchConfidence": ["high", "very high"],
			"groupName": "highConfidence"
		}
	]
}
